{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "per-file": true,
  "lines": 40,
  "statements": 40,
  "functions": 50,
  "branches": 30,
  "extension": [
    ".js",
    ".ts",
    ".tsx"
  ],
  "include": [
    "src/**/!(*.test.*).[tj]s?(x)",
    "scripts/**/!(*.test.*).[tj]s?(x)"
  ],
  "exclude": [
    "**/*.test.js",
    "**/*.test.ts"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "report-dir": "./output/coverage",
  "cache": true,
  "temp-directory": "./temp/nycrc"
}